    ["DictKey_ActionText_235"] = "--despawn\
function groupDespawn ()\
  for groupName, groupData in pairs(mist.DBs.groupsByName) do\
    if string.find(string.lower(groupName), 'spawn') and Group.getByName(groupName) then\
      local ltd = Group.getByName(groupName)\
      for index, unit in pairs (ltd:getUnits()) do\
        local someDeadGuy = mist.Logger:new('exploding', 3)\
        someDeadGuy:info(unit)\
        trigger.action.explosion(unit:getPosition().p,100)\
      end\
    end\
  end\
end\
