function groupRespawnRandom ()\
  for groupName, groupData in pairs(mist.DBs.groupsByName) do\
    local respawn_group = false\
    if string.find(string.lower(groupName), 'randspawn') and next(Group.getByName(groupName):getUnits()) == nil then\
      respawn_group = true\
    end\
    if respawn_group then\
      local yubibomb = math.random(10000)\
      if yubibomb >= numberwang then      \
        mist.respawnInZone(groupName, {'randspawn_east', 'randspawn_west'}, true, 500)\
        trigger.action.outText('THAT\\'S NUMBERWANG - Group \"' .. groupName .. '\" respawned in a random location.', 10, false)\
      end\
    end\
  end\
end",
