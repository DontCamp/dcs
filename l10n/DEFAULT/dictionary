function groupRespawn ()\
  for groupName, groupData in pairs(mist.DBs.groupsByName) do\
    local respawn_group = false\
    if string.find(string.lower(groupName), 'respawn') and next(Group.getByName(groupName):getUnits()) == nil then\
      respawn_group = true\
    elseif string.find(string.lower(groupName), 'respawn') and not Group.getByName(groupName) then\
      respawn_group = true\
    end\
    if respawn_group then\
      local yubibomb = math.random(10000)\
      if yubibomb >= numberwang then      \
        mist.respawnGroup(groupName, true)\
        trigger.action.outText('THAT\\'S NUMBERWANG - Group \"' .. groupName .. '\" respawned.', 10, false)\
      end\
    end\
  end\
end",
